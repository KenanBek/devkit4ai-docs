        "groups": [
          {
            "group": "API documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Health Check",
            "pages": [
              "api-reference/health/check",
              "api-reference/health/database",
              "api-reference/health/storage",
              "api-reference/health/redis",
              "api-reference/health/detailed"
