              "api-reference/projects/delete",
              "api-reference/projects/create-api-key",
              "api-reference/projects/list-api-keys",
              "api-reference/projects/revoke-api-key"
            ]
          },
          {
            "group": "AI Generation",
            "pages": [
              "api-reference/generation/create",
              "api-reference/generation/status",
              "api-reference/generation/list",
              "api-reference/generation/toggle-public",
